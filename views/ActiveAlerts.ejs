<%- include('layout/header') %> <% if(data){%>
<div>
  <h1 style="text-align: center;">Active Alerts</h1>
  <h2 style="text-align: center;">Following are the active alerts</h2>
  <hr />
  <table class="table table-strriped">
    <thead>
      <tr class="">
        <th>Title</th>
        <th>Time</th>
        <th>Date</th>
        <th>Type</th>
      </tr>
    </thead>
    <% data.forEach((item)=>{ %>

    <tr>
      <td><%=item.title %></td>
      <td><%=item.timeInput%></td>
      <td><%=item.AlertDate %></td>
      <td><%=item.AlertType %></td>
    </tr>
    <% }) %>
    <tbody></tbody>
  </table>
</div>
<div style="text-align: center;">
    &copy; 2023 Alert System PVT LTD   
</div>
<%} %> <%- include('layout/footer') %>
